{
    "name": "www",
    "private": true,
    "type": "module",
    "version": "1.0.0",
    "scripts": {
        "astro": "astro",
        "build": "pnpm run build:registry && cross-env REGISTRY_URL=/r astro build",
        "build:registry": "tsx scripts/build-registry.ts",
        "deploy": "pnpm run build && wrangler deploy",
        "dev": "cross-env REGISTRY_URL=/r astro dev",
        "dev:debug": "cross-env DEBUG=true REGISTRY_URL=/r astro dev",
        "preview": "astro preview",
        "type-check": "astro sync && tsc --noEmit"
    },
    "dependencies": {
        "@ariakit/react": "^0.4.17",
        "@astrojs/mdx": "^4.3.9",
        "@astrojs/react": "^4.4.1",
        "@astrojs/sitemap": "3.2.1",
        "@astrojs/starlight": "^0.36.2",
        "@base-ui-components/react": "1.0.0-beta.1",
        "@lucide/astro": "^0.562.0",
        "@radix-ui/react-accordion": "^1.2.4",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-checkbox": "^1.1.5",
        "@radix-ui/react-dialog": "^1.1.7",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-label": "^2.1.3",
        "@radix-ui/react-popover": "^1.1.14",
        "@radix-ui/react-radio-group": "^1.2.4",
        "@radix-ui/react-select": "^2.1.7",
        "@radix-ui/react-slot": "^1.2.0",
        "@radix-ui/react-switch": "^1.1.4",
        "@radix-ui/react-tabs": "^1.1.4",
        "@sugarcube-sh/cli": "workspace:*",
        "@sugarcube-sh/vite": "workspace:*",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.3",
        "astro": "^5.15.3",
        "astro-auto-import": "^0.4.4",
        "astro-robots-txt": "^1.0.0",
        "astro-webmanifest": "^1.0.0",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "cross-env": "^7.0.3",
        "lucide-react": "^0.468.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.55.0",
        "ultrahtml": "^1.6.0"
    },
    "devDependencies": {
        "@unocss/preset-wind3": "^66.6.0",
        "@unocss/preset-wind4": "^66.6.0",
        "tsx": "^4.21.0",
        "wrangler": "^4.20.5"
    }
