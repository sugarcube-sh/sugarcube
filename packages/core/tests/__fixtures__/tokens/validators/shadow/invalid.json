      "blur": { "value": 4, "unit": "px" },
      "spread": { "value": 0, "unit": "px" }
    },
    "$path": "shadow.invalid.dimension.unit",
    "$source": {
      "collection": "tokens",
      "sourcePath": "tokens/shadows.json"
    }
