{
  "shadow.invalid.type": {
    "$type": "shadow",
    "$value": "not-an-object",
    "$path": "shadow.invalid.type",
    "$source": {
      "collection": "tokens",
      "sourcePath": "tokens/shadows.json"
    }
  },
  "shadow.invalid.color": {
